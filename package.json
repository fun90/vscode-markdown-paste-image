{
  "name": "vscode-markdown-paste-image",
  "displayName": "Markdown Paste",
  "description": "A smartly paste for markdown.",
  "version": "0.7.3",
  "publisher": "telesoho",
  "author": {
    "name": "telesoho",
    "email": "telesoho@gmail.com"
  },
  "license": "(MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/telesoho/vscode-markdown-paste-image.git"
  },
  "homepage": "https://github.com/telesoho/vscode-markdown-paste-image",
  "bugs": {
    "url": "https://github.com/telesoho/vscode-markdown-paste-image/issues"
  },
  "icon": "res/icon.png",
  "galleryBanner": {
    "color": "#252526",
    "theme": "dark"
  },
  "keywords": [
    "paste",
    "image",
    "markdown",
    "paste image",
    "paste url"
  ],
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.MarkdownPaste",
    "onCommand:extension.MarkdownRuby"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Markdown Paste Configuration",
      "properties": {
        "pasteImage.path": {
          "type": "string",
          "default": "./",
          "description": "The destination to save image file."
        },
        "pasteImage.prefix": {
          "type": "string",
          "default": "",
          "description": "The string prepend to the resolved image path before paste."
        },
        "pasteImage.suffix": {
          "type": "string",
          "default": "",
          "description": "The string append to the resolved image path before paste."
        },
        "pasteImage.silence": {
          "type": "boolean",
          "default": false,
          "description": "Enable/disable showing confirm box while paste image."
        },
        "MarkdownPaste.rules": {
          "type": "array",
          "default": [
            {
              "regex": "^(https?:\/\/.+\\.)(jpg|jpeg|bmp|gif|png)$",
              "options": "ig",
              "replace": "![]($1$2)"
            },
            {
                "regex": "(.+\\.)(jpg|jpeg|bmp|gif|png)$",
                "options": "ig",
                "replace": "![]($1$2)"
            },
            {
                "regex": "^(https?:\/\/.*)",
                "options": "ig",
                "replace": "[]($1)"
            }
          ],
          "description": "Rules for markdown paste."
        }
      }
    },
    "commands": [
      {
        "command": "extension.MarkdownPaste",
        "title": "Markdown Paste"
      },
      {
        "command": "extension.MarkdownRuby",
        "title": "Markdown Ruby"
      }
    ],
    "keybindings": [
      {
        "command": "extension.MarkdownPaste",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.MarkdownRuby",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "dependencies": {
    "clipboardy": "^1.1.4",
    "moment": "^2.14.1",
    "shelljs": "^0.7.7",
    "to-markdown": "^3.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.1.5"
  }
}
